@model List<ABCDemo2.Models.tblQualifiche>
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewBag.Title = "Admin";
}

@{
    if (Model.Count > 0)
    {
    <table>
        <thead>
            <tr>
                <th>Codice</th>
                <th>Descrizione</th>
            </tr>
        </thead>
        <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.CodiceQualifica</td>
            <td>@item.DescrizioneQualifica</td>
        </tr>
        }
        </tbody>
    </table>
    }
}